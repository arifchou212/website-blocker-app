{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "1.0",
    "description": "Block websites and set timers.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "storage"
    ]
}
